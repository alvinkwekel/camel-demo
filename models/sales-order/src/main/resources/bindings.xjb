<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<bindings xmlns="http://java.sun.com/xml/ns/jaxb" xmlns:xs="http://www.w3.org/2001/XMLSchema"
  xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/jaxb http://java.sun.com/xml/ns/jaxb/bindingschema_2_0.xsd" version="2.1">

  <globalBindings>
    <xjc:javaType name="java.lang.Integer" xmlType="xs:integer" adapter="com.liberition.model.IntegerAdapter" />
    <xjc:javaType name="java.lang.Double" xmlType="xs:decimal" adapter="com.liberition.model.DoubleAdapter" />
    <xjc:javaType name="java.time.ZonedDateTime" xmlType="xs:dateTime" adapter="com.liberition.model.ZonedDateTimeAdapter" />
    <xjc:javaType name="java.time.LocalDate" xmlType="xs:date" adapter="com.liberition.model.LocalDateAdapter" />
    <xjc:javaType name="java.time.LocalTime" xmlType="xs:time" adapter="com.liberition.model.LocalTimeAdapter" />
  </globalBindings>
</bindings>
